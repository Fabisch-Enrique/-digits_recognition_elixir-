<div id="wrapper" phx-update="ignore">
    <div id="canvas" phx-hook="Draw"></div>
</div>
<div>
    <button phx-click="reset">RESET</button>
    <button phx-click="predict">PREDICT</button>
</div>

<%= if @prediction do %>
<div>
    <div>Prediction:</div>
    <div><%= @prediction %></div>
</div>
<% end %>